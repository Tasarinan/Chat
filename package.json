    "lodash": "^4.17.5",
    "lowdb": "^1.0.0",
    "mobx": "^3.3.1",
    "mobx-logger": "^0.6.0",
    "mobx-react": "^4.3.3",
    "mobx-react-form": "^1.32.2",
    "mobx-react-router": "^4.0.1",
    "moment": "^2.19.1",
    "pbkdf2": "^3.0.14",
    "quill": "^1.3.6",
    "rc-util": "^4.3.1",
