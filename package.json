{
  "name": "e-mobile",
  "author": "Oliver",
  "productName": "e-mobile",
  "version": "0.9.2",
  "description": "nb chat tool",
  "main": "./src/entry.js",
  "license": "MIT",
  "build": {
    "publish": {
      "provider": "generic",
      "url": "http://192.168.82.163:1337/update/${os}_${arch}",
      "channel": "latest"
    },
    "appId": "com.weaver.e-mobile",
    "copyright": "WEAVER",
    "buildDependenciesFromSource": true,
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "./src/electron-main/images/logo_256.png"
    },
    "mac": {
      "icon": "./src/electron-main/images/logo.icns"
    },
    "extraFiles": [
      "dll"
    ]
  },
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint --ext .js,.jsx src test",
    "start": "webpack-dev-server --color --config ./config/webpack.config.dev.js --mode development ",
    "build": "webpack --config ./config/webpack.config.prod.js --mode production",
    "ele": "electron . --debug",
    "compile:ele": "node scripts/buildEle",
    "pack": "build --dir",
    "dist": "build -p always -w --x64",
    "dist:mac": "build -p always -m --x64",
    "new-build": "yarn compile:ele && yarn dist",
    "new-buildm": "yarn compile:ele && yarn dist:mac",
    "builder": "yarn build && yarn compile:ele && yarn dist",
    "builderm": "yarn build && yarn compile:ele && yarn dist:mac"
  },
  "homepage": "/client/main",
  "repository": "",
  "dependencies": {
    "antd": "^3.4.1",
    "auto-launch": "^5.0.5",
    "axios": "^0.17.1",
    "electron-is": "^2.4.0",
